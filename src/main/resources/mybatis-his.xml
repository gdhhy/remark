<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="cacheEnabled" value="true"/>
        <setting name="lazyLoadingEnabled" value="false"/>
        <setting name="aggressiveLazyLoading" value="true"/>
    </settings>

    <typeHandlers>
        <typeHandler handler="com.zcreate.mybatis.JsonElementTypeHandler"/>
    </typeHandlers>

    <mappers>
        <mapper resource="com/zcreate/remark/model/his/PatientInfo.xml"/>
        <mapper resource="com/zcreate/remark/model/check/Check.xml"/>
    </mappers>
</configuration>
